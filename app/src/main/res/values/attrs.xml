<resources>
    <declare-styleable name="FlowLayout">
        <attr name="horizontalSpacing" format="dimension" />
        <attr name="verticalSpacing" format="dimension" />
    </declare-styleable>
    <declare-styleable name="FlowLayout_LayoutParams">
        <attr name="layout_horizontalSpacing" format="dimension" />
    </declare-styleable>

    <declare-styleable name="SeekBarDialogPreference">
        <attr name="android:max" />
        <attr name="min" format="integer" />
        <attr name="progressTextSuffix" format="string" />
    </declare-styleable>

    <declare-styleable name="ViewTopMenu">
        <attr name="menuType" format="integer" />
    </declare-styleable>

    <declare-styleable name="ProgressBar">
        <attr name="android:progressDrawable" />
        <attr name="android:minWidth" />
        <attr name="android:maxWidth" />
        <attr name="android:minHeight" />
        <attr name="android:maxHeight" />
        <attr name="android:max" />
        <attr name="android:progress" />
        <attr name="android:secondaryProgress" />
    </declare-styleable>

    <declare-styleable name="LabelsView">
        <attr name="max_labels" format="integer" />
        <attr name="show_cross" format="boolean" />
        <attr name="icon_cross" format="reference" />
        <attr name="text_color" format="color"/>
        <attr name="text_size" format="dimension"/>
        <attr name="label_background_res" format="reference"/>
        <attr name="label_clickable" format="boolean" />
        <attr name="label_padding" format="dimension"/>
    </declare-styleable>
</resources>
